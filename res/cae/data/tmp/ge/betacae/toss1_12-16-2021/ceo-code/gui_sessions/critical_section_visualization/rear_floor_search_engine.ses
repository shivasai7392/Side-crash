opt var string f21_rear_floor_hes 655*,656*,657*
opt var string f21_rear_floor_hes_exceptions null
opt var string f21_rear_floor_erase_pids null
opt var string f21_rear_floor_transparent_pids 1533,1553,1559,1615,1616,1620,1621,1624
opt var string f21_rear_floor_view top
opt var string f21_rear_floor_name REAR_FLOOR
opt var string f21_rear_floor_erase_box -10000,-10000,-10000,170,10000,10000

$-- F21 FRONT FLOOR
opt var add search ${f21_rear_floor_hes}
opt var add hes_exceptions ${f21_rear_floor_hes_exceptions}
opt var add exclude null
opt var add erase_pids ${f21_rear_floor_erase_pids}
opt var add erase_box ${f21_rear_floor_erase_box}
opt var add comp_view ${f21_rear_floor_view}
opt var add transparent_pids ${f21_rear_floor_transparent_pids}
opt var add comp_name "${f21_rear_floor_name}"
opt var add file_name "${f21_rear_floor_name}"

or advfilter partoutput add:Parts:name:${search}:Keep All
add pid ${hes_exceptions}
erase advfilter partoutput add:Parts:name:${exclude}:Keep All
erase pid ${erase_pids}
erase shells box ${erase_box}
erase solids box ${erase_box}
view default ${comp_view}
view center
color pid transparency ${transparency_level} ${transparent_pids}
lock store "${comp_name}"