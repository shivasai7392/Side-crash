opt var string f21_upb_inner_hes 645*,646*
opt var string f21_upb_inner_hes_exceptions null
opt var string f21_upb_inner_erase_pids null
opt var string f21_upb_inner_transparent_pids  2217,2223-2224,2226,2230
opt var string f21_upb_inner_view right
opt var string f21_upb_inner_name UPR_INNER
opt var string f21_upb_inner_erase_box -10000,0,-10000,10000,100000,10000

$-- F21 UPB OUTER
opt var add search ${f21_upb_inner_hes}
opt var add hes_exceptions ${f21_upb_inner_hes_exceptions}
opt var add exclude null
opt var add erase_pids ${f21_upb_inner_erase_pids}
opt var add erase_box ${f21_upb_inner_erase_box}
opt var add comp_view ${f21_upb_inner_view}
opt var add transparent_pids ${f21_upb_inner_transparent_pids}
opt var add comp_name "${f21_upb_inner_name}"
opt var add file_name "${f21_upb_inner_name}"

or advfilter partoutput add:Parts:name:${search}:Keep All
add pid ${hes_exceptions}
erase advfilter partoutput add:Parts:name:${exclude}:Keep All
erase pid ${erase_pids}
erase shells box ${erase_box}
erase solids box ${erase_box}
view default ${comp_view}
view center
color pid transparency ${transparency_level} ${transparent_pids}
lock store "${comp_name}"