opt var string f21_front_floor_hes 651*,653*
opt var string f21_front_floor_hes_exceptions null
opt var string f21_front_floor_erase_pids 1625-1633,
opt var string f21_front_floor_transparent_pids 1636,1644,1657-1658,1669,1670,1679,1685
opt var string f21_front_floor_view top
opt var string f21_front_floor_name FRONT_FLOOR
opt var string f21_front_floor_erase_box null

$-- F21 FRONT FLOOR
opt var add search ${f21_front_floor_hes}
opt var add hes_exceptions ${f21_front_floor_hes_exceptions}
opt var add exclude null
opt var add erase_pids ${f21_front_floor_erase_pids}
opt var add erase_box "${f21_front_floor_erase_box}"
opt var add comp_view ${f21_front_floor_view}
opt var add transparent_pids "${f21_front_floor_transparent_pids}"
opt var add comp_name "${f21_front_floor_name}"
opt var add file_name "${f21_front_floor_name}"

or advfilter partoutput add:Parts:name:${search}:Keep All
add pid ${hes_exceptions}
erase advfilter partoutput add:Parts:name:${exclude}:Keep All
erase pid ${erase_pids}
erase shells box ${erase_box}
erase solids box ${erase_box}
view default ${comp_view}
view center
color pid transparency ${transparency_level} ${transparent_pids}
lock store "${comp_name}"