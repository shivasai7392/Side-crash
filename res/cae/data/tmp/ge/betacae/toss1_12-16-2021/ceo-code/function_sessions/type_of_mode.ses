# PYTHON script
import os
from meta import *

def main():

	test_mode = utils.MetaGetVariable('verification_mode')
	mode_source = utils.MetaGetVariable('mode_source_path')
	if (test_mode == 'suv_mdb_50kmh') or (test_mode=='ssm_suv_mdb') or (test_mode=='214_mdb_62kmh') or (test_mode == '32kmh_214_pole_am50') or (test_mode=='32kmh_214_pole_af5'):
		session_file = 'side_cae_phase1_v3p0.ses'
		utils.MetaSetVariable('phase1_session_file',session_file)
	print('SOURCED PHASE 1 SESSION : '+os.path.join(mode_source,session_file))

if __name__ == '__main__':
	main()

