xyplot read ascii column "${attack_window}" "${bolt_source_path}M14_C10p9_ServiceLoad.csv" 1 2 0
xyplot curve set name "${attack_window}" ${LAST_CURVE_ID} "RAW SERVICE"
xyplot curve rfunction newscale all "${attack_window}" ${LAST_CURVE_ID} ${scale}
opt var add service_load_curve ${LAST_CURVE_ID}

xyplot curve set color "${attack_window}" ${service_load_curve} Green
xyplot curve set linewidth "${attack_window}" ${service_load_curve} 1
xyplot curve set name "${attack_window}" ${service_load_curve} "SERVICE-LOAD"

xyplot curve function area options direction xaxis
xyplot curve function area insert "" "" ${100_per_curve} ${90_per_curve} Red "${attack_window}"
xyplot curve function area options direction yaxis
xyplot curve function area insert "" "" ${100_per_curve} ${90_per_curve} Red "${attack_window}"

xyplot curve function area options direction xaxis
xyplot curve function area insert "" "" ${90_per_curve} 0 Yellow "${attack_window}"

xyplot curve function area options direction yaxis
xyplot curve function area insert "" "" ${service_load_curve} 0 Green "${attack_window}"