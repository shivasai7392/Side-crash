read session /cae/data/reference/fr2/_DevelopmentCAE/04_Scripts/phase1_sessions/function_sessions/src/2d_plot_formatting.ses

$-- SAVING OUT CSV FILES
xyplot plotactive "${formatting_window_name}" 0-1
