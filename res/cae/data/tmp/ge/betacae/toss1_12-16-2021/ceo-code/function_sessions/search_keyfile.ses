//#!python
# PYTHON script
import meta
import os
import string
import subprocess as sub
from meta import utils

def main():
	sim_data = []
	source_path = utils.MetaGetVariable('project_path')
	command_line = "grep 'export INFILE' " + source_path + "*_met.qscript | sed -e s/^'export INFILE='//"
	cmd_data=sub.Popen(command_line, shell=True, stdout=sub.PIPE, stderr=sub.PIPE)
	(out,err)=cmd_data.communicate()
	out = str(out)
	line = out.split("'")[1]
	size = len(line)
	master_file = line[:size - 2]
	utils.MetaSetVariable('master_file', master_file)
if __name__ == '__main__':
	main()
//#!EOF