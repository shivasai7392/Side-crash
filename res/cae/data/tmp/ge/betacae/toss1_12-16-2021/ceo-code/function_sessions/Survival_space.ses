$-- SURVIVAL SPACE -- Z VS Y -- INTRUSION [mm] EVERY 10ms -- Y IS SCALED AND THEN OFFSET
$
$ opt var add d3plot_data MetaPost
$ opt var add model 0
$ opt var add window_id 3
$ opt var add scale "-1"
$ opt var add offset "420"
$ opt var string follow_nodes 7,8,9
$ opt var add window "Survival Space"
$ opt var string name SKIN_HIP_
$ opt var string bplr_ss_nodes "40002318/40001387/40001149/2197371/2197226/2197850/2198123/2209213/2214494/2214714/2214871/6075711/6075726/6075740/2215320/2215460/2215599/2215751/2215946/2216118/2216256/6076944/6833972"
$ MAKE SURE bplr_ss_nodes IS SORTED IN Y

//#!python
# PYTHON script
import meta
from meta import *

def main():

	print("---Survival space >> Z vs Y offset by HP")
	print("")

if __name__ == '__main__':
	main()
